{
    "name": "houzhouyang/hyperf-operation-log",
    "type": "library",
    "description": "Hyperf operation log component",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "operation-log",
        "component"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=8.0",
        "hyperf/config": "^2.2 || ^3.0",
        "hyperf/contract": "^2.2 || ^3.0",
        "hyperf/database": "^2.2 || ^3.0",
        "hyperf/di": "^2.2 || ^3.0",
        "hyperf/event": "^2.2 || ^3.0",
        "hyperf/http-server": "^2.2 || ^3.0",
        "hyperf/logger": "^2.2 || ^3.0",
        "hyperf/async-queue": "^2.2 || ^3.0",
        "hyperf/redis": "^2.2 || ^3.0"
    },
    "require-dev": {
        "hyperf/devtool": "^2.2 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "HyperfOperationLog\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "HyperfOperationLog\\ConfigProvider"
        }
    }
}
